{
  "stryker-config": {
    "project": "src/ConstraintMcpServer/ConstraintMcpServer.csproj",
    "test-projects": [
      "tests/ConstraintMcpServer.Tests/ConstraintMcpServer.Tests.csproj"
    ],
    "reporters": [
      "progress",
      "html",
      "json"
    ],
    "mutation-level": "Standard",
    "thresholds": {
      "high": 80,
      "low": 70,
      "break": 70
    },
    "mutate": [
      "src/ConstraintMcpServer/Application/Selection/KeywordMatcher.cs",
      "src/ConstraintMcpServer/Application/Selection/ConstraintSelector.cs",
      "src/ConstraintMcpServer/Domain/ConstraintFactory.cs",
      "src/ConstraintMcpServer/Domain/Constraint.cs"
    ],
    "additional-timeout": 60000,
    "coverage-analysis": "perTest",
    "concurrency": 4,
    "disable-mix-mutants": true,
    "disable-bail": false,
    "verbosity": "info"
  }
}